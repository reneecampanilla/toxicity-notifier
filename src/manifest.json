{
  "name": "Toxicity Notifier",
  "description": "A chrome extension that detects toxicity level of posts in your newsfeed.",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
    "48": "icons/poison.png",
    "128": "icons/poison.png"
  },
  "browser_action": {
    "default_title": "extension",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  },
  "permissions": ["http://www.google.com/", "https://www.google.com/", "https://storage.googleapis.com/*"]
}
